{
  "name": "form-elements",
  "description": "Collection of form elements for Angular 6+ projects.",
  "version": "2.2.1",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/form-elements.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/form-elements/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "postinstall": "ts-node storybook-patch.ts",
    "postbuild": "yarn run build-scss && yarn run copy-metadata",
    "start": "ng serve form-elements-demo",
    "build": "ng build --prod",
    "build-scss": "scss-bundle -e projects/form-elements/src/lib/main.scss -d dist/form-elements/styles.scss --includePaths node_modules",
    "copy-metadata": "cp README.md dist/form-elements/README.md && cp LICENSE.md dist/form-elements/LICENSE.md && cp CHANGELOG.md dist/form-elements/CHANGELOG.md",
    "lint": "ng lint form-elements --fix",
    "format": "prettier --write \"{src,projects}/**/*.{ts,scss,md,html}\"",
    "changelog": "standard-changelog --release-count 1",
    "storybook": "start-storybook --port 9001 --config-dir .storybook --static-dir node_modules --ci",
    "docs": "build-storybook --config-dir .storybook --output-dir docs",
    "link": "yarn run build && cd dist/form-elements && yarn link",
    "unlink": "cd dist/form-elements && yarn unlink"
  },
  "greenkeeper": {
    "ignore": [
      "core-js",
      "typescript",
      "zone.js"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@pascaliske/html-helpers": "^1.5.1",
    "core-js": "^2.6.5",
    "rxjs": "^6.5.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular-devkit/build-ng-packagr": "~0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^8.0.0",
    "@babel/core": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@pascaliske/schematics": "^0.0.5",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/angular": "^5.0.11",
    "@types/node": "^12.0.8",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/webpack": "^4.4.32",
    "@types/webpack-env": "^1.13.9",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "codelyzer": "~5.1.0",
    "flatpickr": "4.6.1",
    "fuzzysearch": "^1.0.3",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "ng-packagr": "^5.3.0",
    "prettier": "^1.18.2",
    "scss-bundle": "^2.5.1",
    "standard-changelog": "^2.0.11",
    "style-loader": "^0.23.1",
    "ts-node": "~8.2.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.10.0",
    "tslint": "~5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  }
}
