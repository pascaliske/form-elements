{
  "name": "form-elements",
  "description": "Collection of form elements for Angular 6+ projects.",
  "version": "2.4.4",
  "license": "MIT",
  "homepage": "https://pascaliske.github.io/form-elements",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/form-elements.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/form-elements/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "postbuild": "yarn run build-scss && yarn run copy-metadata",
    "start": "ng serve form-elements-demo",
    "build": "ng build --prod",
    "build-scss": "scss-bundle",
    "copy-metadata": "cp README.md dist/form-elements/README.md && cp LICENSE.md dist/form-elements/LICENSE.md && cp CHANGELOG.md dist/form-elements/CHANGELOG.md",
    "lint": "ng lint form-elements --fix",
    "format": "prettier --write \"{src,projects}/**/*.{ts,scss,md,html}\"",
    "version": "bump --yes package.json projects/form-elements/package.json",
    "changelog": "standard-changelog",
    "storybook": "start-storybook --port 9001 --config-dir .storybook --static-dir node_modules --ci",
    "docs": "build-storybook --config-dir .storybook --output-dir docs",
    "link": "yarn run build && cd dist/form-elements && yarn link",
    "unlink": "cd dist/form-elements && yarn unlink"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@pascaliske/html-helpers": "^1.5.7",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "^9.0.2",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@babel/core": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@pascaliske/schematics": "^0.0.8",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/angular": "^5.3.13",
    "@storybook/theming": "^5.3.13",
    "@types/node": "^13.7.1",
    "@types/webpack": "^4.41.6",
    "@types/webpack-env": "^1.15.1",
    "babel-loader": "^8.0.6",
    "bump-cli": "^1.1.3",
    "chalk": "^3.0.0",
    "codelyzer": "^5.2.1",
    "flatpickr": "4.6.3",
    "fuzzysearch": "^1.0.3",
    "husky": "^4.2.2",
    "lint-staged": "^10.0.7",
    "ng-packagr": "^5.7.1",
    "prettier": "^1.19.1",
    "scss-bundle": "^3.0.2",
    "standard-changelog": "^2.0.21",
    "style-loader": "^1.1.3",
    "ts-node": "^8.6.2",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5"
  }
}
