{
  "name": "form-elements",
  "description": "Collection of form elements for Angular 6+ projects.",
  "version": "2.3.0",
  "license": "MIT",
  "homepage": "https://pascaliske.github.io/form-elements",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/form-elements.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/form-elements/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "postbuild": "yarn run build-scss && yarn run copy-metadata",
    "start": "ng serve form-elements-demo",
    "build": "ng build --prod",
    "build-scss": "scss-bundle -e projects/form-elements/src/lib/main.scss -d dist/form-elements/styles.scss --includePaths node_modules",
    "copy-metadata": "cp README.md dist/form-elements/README.md && cp LICENSE.md dist/form-elements/LICENSE.md && cp CHANGELOG.md dist/form-elements/CHANGELOG.md",
    "lint": "ng lint form-elements --fix",
    "format": "prettier --write \"{src,projects}/**/*.{ts,scss,md,html}\"",
    "changelog": "standard-changelog --release-count 1",
    "storybook": "start-storybook --port 9001 --config-dir .storybook --static-dir node_modules --ci",
    "docs": "build-storybook --config-dir .storybook --output-dir docs",
    "link": "yarn run build && cd dist/form-elements && yarn link",
    "unlink": "cd dist/form-elements && yarn unlink"
  },
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.10.1",
    "@fortawesome/free-regular-svg-icons": "^5.10.1",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@pascaliske/html-helpers": "^1.5.7",
    "core-js": "^3.2.1",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular-devkit/build-ng-packagr": "~0.802.2",
    "@angular/cli": "^8.2.2",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "^8.2.2",
    "@babel/core": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@pascaliske/schematics": "^0.0.8",
    "@storybook/addon-actions": "^5.2.0-beta.36",
    "@storybook/addon-knobs": "^5.2.0-beta.33",
    "@storybook/addon-links": "^5.2.0-beta.33",
    "@storybook/addon-notes": "^5.2.0-beta.36",
    "@storybook/angular": "^5.2.0-beta.36",
    "@types/node": "^12.7.2",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/webpack": "^4.39.0",
    "@types/webpack-env": "^1.14.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "codelyzer": "^5.0.1",
    "flatpickr": "4.6.2",
    "fuzzysearch": "^1.0.3",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "ng-packagr": "^5.5.0",
    "prettier": "^1.18.2",
    "scss-bundle": "^2.5.1",
    "standard-changelog": "^2.0.13",
    "style-loader": "^0.23.1",
    "ts-node": "^8.3.0",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5"
  }
}
